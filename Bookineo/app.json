{
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "expo-toast",
          "geolib",
          "react-native-vector-icons"
        ],
        "listUnknownPackages": false
      }
    },

    "name": "Bookineo",
    "slug": "Bookineo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "Bookineo/src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Bookineo a besoin d'accéder à votre position pour afficher les boîtes à livres près de chez vous."
        }
      ]
    ],
    "splash": {
      "image": "Bookineo/src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bookineo.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Bookineo a besoin d'accéder à votre position pour afficher les boîtes à livres près de chez vous."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "Bookineo/src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bookineo.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cb54ee19-02bb-48d1-97fa-1c7af44a9588"
      }
    }
  }
}
